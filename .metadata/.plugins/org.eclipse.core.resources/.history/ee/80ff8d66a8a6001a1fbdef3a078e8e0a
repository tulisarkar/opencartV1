
public class arraydemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		{
			int arr[]=new int[5];
			arr[0]=12;
			arr[1]=23;
			arr[2]=44;
			arr[3]=56;
			arr[4]=78;
			
			for(int i=arr.length-1;i>=arr.length-1;i--){

			System.out.println(arr[i]);

			}

			}
			}

	}


